{
    "name": "windlang",
    "displayName": "Windlang",
    "description": "Syntax highlighting for Windlang",
    "version": "0.1.6",
    "icon": "assets/WindLogo.png",
    "publisher": "YoussefAhmed",
    "repository": {
        "url": "https://github.com/joetifa2003/windlang"
